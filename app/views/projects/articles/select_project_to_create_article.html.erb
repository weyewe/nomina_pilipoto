
<div class="span9 content">
  
  <%= create_guide("Create Article", 
        "Migrate from Finalized Project, or Create New Article").html_safe %>
  
  <%#= create_breadcrumb(@breadcrumbs).html_safe %>
  
  <%= render :partial => "articles/article_source_selector", :locals => {
    :is_migrate_from_project => @is_migrate_from_project
  }%>
  
  

  <div class="row-fluid">
    <!-- <div class="span12"> -->

      <table cellpadding="0" cellspacing="0" border="0" 
            class="table table-striped table-bordered" id="example">
        <thead>
          <tr>
            <th>Project Name</th>
            <th>Approved Pictures</th>
            <th colspan='3' style="text-align:center;">Action</th> <!-- Change Title, Order Images, pull down-->
          </tr>
        </thead>
        <tbody>
          <% @projects.each do |project| %>
            <%= render :partial =>"projects/article_from_project", :locals => {
              :project => project,
              :article => project.article
            }%>
          <% end %>
          


        </tbody>
      </table>



    <!-- </div> -->   <!--/span12-->
  </div><!--/row-->


  <div class=" hidden" id="close-entry">
    <%= form_tag  create_article_from_project_url  , :method => :post  do  %>
      <%= hidden_field_tag 'entry_id' , ''  %>
    <% end %>
  </div>
  
  <!-- <div class=" hidden" id="article-publisher">
     <%#= form_tag  execute_publish_project_based_article_url  , :method => :post  do  %>
       <%#= hidden_field_tag 'entry_publish_id' , ''  %>
       <%#= hidden_field_tag 'publisher_action' , ''  %>
     <%# end %>
   </div> -->
 

</div><!--/span-->

<a id="header_pane"></a>
 <!--/ header -->

 <div id="slide_info_container" class="post_item">
   <!--Slide captions displayed here-->
   <div id="slidecaption"><h3></h3></div>
   <div class="clear"></div>
   <!--Slide description displayed here-->
   <div id="slidedescription" class="entry"></div>
   <div id="slide_info_toggle"></div>
 </div>
 










